{
   "extends": ["next", "next/core-web-vitals", "plugin:prettier/recommended"],
   "plugins": ["prettier", "import"],
   "rules": {
      "import/order": [
         "error",
         {
            "groups": [
               ["builtin", "external"],
               ["internal", "parent", "sibling", "index"]
            ],
            "newlines-between": "always",
            "alphabetize": {
               "order": "asc",
               "caseInsensitive": true
            }
         }
      ]
   }
}
